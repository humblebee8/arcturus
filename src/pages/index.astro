---
import Default from "../layouts/Default.astro";
import { Markdown } from 'astro/components';

const content = await fetch(
	`${import.meta.env.PUBLIC_CONTENT_URL}pages/index.md`
).then(res => res.text());
---
<Default title="welcome to Arcturus" description="Arcturus">
	<Markdown>
		{content}
	</Markdown>
</Default>
